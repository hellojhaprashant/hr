#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char *s;
    s = malloc(1024 * sizeof(char));
    scanf("%[^\n]", s);
    s = realloc(s, strlen(s) + 1);
    //Write your logic to print the tokens of the sentence here.
    //printf("%c", s[1]);
    for (int i=0; i<strlen(s); i++){
        char c = s[i];
        if (c == ' '){
            printf("\n");
        }else{
            printf("%c", c);
        }
    }
    
    return 0;
}
